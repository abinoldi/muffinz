        if not STATE.HasAstralRod then
            GUI.StrategyText.Text = "STRATEGY: Priority ‚Üí Astral Rod (1M Coins)"
            GUI.StrategyText.TextColor3 = Color3.fromRGB(255, 215, 0)
            strategyBox.BackgroundColor3 = Color3.fromRGB(70, 40, 30)

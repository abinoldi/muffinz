-- ===============================
print("[System] ULTIMATE INVISIBLE EDITION Loading...")

-- Auto-start all features based on your config
task.delay(2, function()
    -- Start Invisible Potato Mode immediately
    PotatoModeEnabled = true
    StartInvisiblePotatoLoop()
    PotatoLabel.Text = "Invisible Mode: ACTIVE"
    
    -- Start Auto Totem (3700 detik)
    AutoTotemEnabled = true
    StartAutoTotemLoop()
    
    -- Start Fishing at Crater Island
    FishingConfig = {Enabled = true, Area = "Crater Island"}
    if AreaData["Crater Island"] then
        local data = AreaData["Crater Island"]
        local char = lp.Character
        if char then
            local root = char:FindFirstChild("HumanoidRootPart")
            if root then
                root.CFrame = CFrame.new(data.Pos, data.Pos + data.Look)
                task.wait(1)
            end
        end
    end
    StartFishingV1()
    LocationLabel.Text = "Location: Crater Island"
    FishLabel.Text = "Auto Fishing: true"
    
    -- Start Trading
    TradingConfig = {AutoTrade = true, TargetUsername = "mm3kenak", Delay = 6}
    task.spawn(function()
        local s, id = pcall(function() return Players:GetUserIdFromNameAsync("mm3kenak") end)
        if s then 
            TradingConfig.TargetId = id
            RunAutoTrade()
        end
    end)
    TradeLabel.Text = "Auto Trade: true"
    
    print("[System] ALL SYSTEMS ACTIVE:")
    print("- Invisible Potato Mode: ON")
    print("- Auto Totem: EVERY 3700s")
    print("- Auto Fishing: Crater Island")
    print("- Auto Trade: mm3kenak")
end)


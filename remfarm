-- ======================================================
-- # ULTIMATE HUB: CONFIGURATION
-- ======================================================
local MasterConfig = {
    -- [GUI SETTINGS]
    DefaultGuiVisible = false,       -- Set ke false agar GUI tersembunyi saat start (Gunakan tombol HUB)

    -- [FISHING SETTINGS]
    FishingEnabled = true,           -- Langsung mulai memancing saat script di-load
    AutoEquip = true,                -- Otomatis spam equip alat pancing
    CatchDelay = 1.5,                -- STRICT 1.5s delay (Sesuai source file)
    AutoSell = true,                 -- Otomatis jual ikan setiap ~65 detik
    AutoFavorite = true,             -- Otomatis kunci Ruby, Leviathan, & Rage

    -- [TELEPORT SETTINGS]
    AutoTeleport = true,             -- Teleport otomatis saat script mulai
    SelectedLocation = "Crater Island", -- Ganti nama lokasi di sini (Lihat daftar di bawah)

    -- [UTILITIES]
    AutoTotem = true,                -- Pasang Mutation Totem otomatis tiap 63 menit
    PotatoMode = true,               -- Mode Invisible/Low Graphic Extreme
    
    -- [TRADING]
    AutoTrade = false,               -- Fitur kirim ikan otomatis ke akun lain
    TargetUsername = "mm3kenak",     -- Nama target untuk trade
    TradeMode = "SECRETS_FIRST",     -- Mode: SECRETS_FIRST, RUBY_GEMSTONE, ALL
    TradeDelay = 6.5,                -- Jeda antar trade

    -- [WEBHOOK]
    WebhookEnabled = true,           -- Mengirim data inventory ke server
    WebhookUrl = "http://43.134.116.2:5000/webhook/fishit", -- URL dari file asli
}


    Title = "Complete Delay", 
    Value = tostring(Config.CompleteDelay), 
    Placeholder = "0.705", 
    Numeric = true,
    Callback = function(text) 
        local n = tonumber(text)
        if n then Config.CompleteDelay = n end
    end 
})

SectFish:Input({ 
    Title = "Cancel Delay", 
    Value = tostring(Config.CancelDelay), 
    Placeholder = "0.346", 
    Numeric = true,
    Callback = function(text) 
        local n = tonumber(text)
        if n then Config.CancelDelay = n end
    end 
})

-- TOTEM TAB
local SectTotem = TabTotem:Section({ Title = "Formation", TextSize = 17 })
SectTotem:Dropdown({ 
    Title="Type", 
    Values={"Luck Totem", "Mutation Totem", "Shiny Totem"}, 
    Value=Config.TotemType, 
    Callback=function(v) Config.TotemType=v end 
})
TotemToggleUI = SectTotem:Toggle({ 
    Title = "Start Formation", 
    Callback = function(s) 
        AUTO_9_TOTEM_ACTIVE=s
        if s then Run9TotemLoop() else if AUTO_9_TOTEM_THREAD then task.cancel(AUTO_9_TOTEM_THREAD) end DisableV3Physics() end 
    end 
})


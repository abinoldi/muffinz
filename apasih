local SectFish = TabFish:Section({ Title = "Fishing", TextSize = 17 })

SectFish:Toggle({ 
    Title = "Enable Blatant", 
    Value = (Config.BlatantEnabled == 1), 
    Callback = function(v) 
        Config.BlatantEnabled = v and 1 or 0 -- Convert Boolean to 1/0
        ToggleBlatant(v)
    end 
})


-- 3. MERCHANT TAB
local SectMerch = TabMerch:Section({ Title = "Merchant Control", TextSize = 17 })
local MerchDisplay = SectMerch:Paragraph({ Title = "Status", Desc = "Turn on Sync to see stock." })
SectMerch:Toggle({
    Title = "Sync Stock", Desc = "Updates list below.", Value = false,
    Callback = function(state)
        if state then
            MerchUpdateThread = task.spawn(function()
                while true do
                    local items = GetMerchantDetails()
                    local str = "--- STOCK ---"
                    if #items == 0 then str = str .. "\n(Out of Stock)" else
                        for _, i in ipairs(items) do str = str .. "\n" .. i.Name .. " (" .. FormatNumber(i.Price) .. ")" end
                    end
                    MerchDisplay:SetDesc(str)
                    task.wait(1)
                end
            end)
        else
            if MerchUpdateThread then task.cancel(MerchUpdateThread) end
            MerchDisplay:SetDesc("Sync Off")
        end
    end
})
SectMerch:Toggle({
    Title = "Auto Buy All", Desc = "Buys everything in stock.", Value = Config.MerchantAutoBuy,
    Callback = function(state)
        Config.MerchantAutoBuy = state; SaveSettings()
        if state then RunAutoBuyMerch() else if AutoBuyMerchThread then task.cancel(AutoBuyMerchThread) end end
    end
})
SectMerch:Button({ Title = "Teleport to Merchant", Callback = function()
    local pos = Vector3.new(-127.7, 2.7, 2759.0)
    if LocalPlayer.Character then LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(pos) end
end})


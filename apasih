        AUTO_9_TOTEM_ACTIVE = state
        if state then Run9TotemLoop() else 
            if AUTO_9_TOTEM_THREAD then task.cancel(AUTO_9_TOTEM_THREAD) end
            DisableV3Physics()
        end
    end

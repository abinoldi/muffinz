local RF_PurchaseMarketItem = GetRemote("RF/PurchaseMarketItem")

-- ==========================================================
--  [FEATURE] MONITORING SCRIPT (YOUR LOGIC)
-- ==========================================================
local WebhookThread = nil

local function GetFishNameAndRarity(item)
    local name = item.Identifier or "Unknown"
    local rarity = item.Metadata and item.Metadata.Rarity or "COMMON"
    
    if ItemUtility and item.Id then
        pcall(function()
            local d = ItemUtility:GetItemData(item.Id)
            if not d then d = ItemUtility:GetItemData(tonumber(item.Id)) end
            if d and d.Data then name = d.Data.Name end
        end)
    end
    
    if item.Metadata and item.Metadata.Rarity then
        rarity = item.Metadata.Rarity
    elseif TierUtility then 
        -- Fallback for rarity calculation
        -- (Simplified for speed in monitoring loop)
    end
    return name, rarity
end

local function SendDataToFlask(summary)
    if Config.WebhookUrl == "" then return end
    
    local json = HttpService:JSONEncode(summary)
    local encoded = HttpService:UrlEncode(json)
    local url = Config.WebhookUrl .. "?player=" .. HttpService:UrlEncode(LocalPlayer.Name) .. "&data=" .. encoded
    
    local ok, err = pcall(function()
        game:HttpGet(url)
    end)
    
    if ok then
        WindUI:Notify({ Title = "Monitor", Content = "Data Sent to Server", Icon = "upload-cloud", Duration = 2 })
    else
        warn("[Monitor] Failed: " .. tostring(err))
    end
end

local function ToggleMonitoring(state)
    Config.WebhookEnabled = state
    SaveSettings()
    
    if state then
        WebhookThread = task.spawn(function()
            WindUI:Notify({ Title = "Monitor", Content = "Scanning started...", Icon = "activity" })
            while Config.WebhookEnabled do
                local r = Replion:WaitReplion("Data", 5)
                if r then
                    local s, data = pcall(function() return r:GetExpect("Inventory") end)
                    if s and data then
                        local summary = {}
                        local total = 0
                        
                        -- Recursive scan for nested inventory tables
                        local function scan(t)
                            for k, v in pairs(t) do
                                if type(v) == "table" and v.Id then -- It's an item
                                    local name, rarity = GetFishNameAndRarity(v)
                                    if rarity == "SECRET" then
                                        summary[name] = (summary[name] or 0) + 1
                                        total = total + 1
                                    end
                                elseif type(v) == "table" then -- It's a folder
                                    scan(v)
                                end
                            end
                        end
                        
                        if data.Items then scan(data.Items) else scan(data) end
                        
                        if total > 0 then
                            SendDataToFlask(summary)
                        end
                    end
                end
                task.wait(Config.WebhookInterval)
            end
        end)
    else
        if WebhookThread then task.cancel(WebhookThread) end
        WindUI:Notify({ Title = "Monitor", Content = "Stopped.", Icon = "x" })
    end
end

-- ==========================================================
--  [FEATURE] GRAPHICS MENU (EXTREME POTATO)
-- ==========================================================
local PotatoLoop = nil

local function ApplyPotato()
    -- Nuke Lighting
    Lighting.GlobalShadows = false
    Lighting.Brightness = 1
    Lighting.FogEnd = 9e9
    Lighting.ClockTime = 12
    for _, v in ipairs(Lighting:GetChildren()) do
        if v:IsA("PostEffect") or v:IsA("Atmosphere") then v:Destroy() end
    end
    
    -- Nuke Terrain
    local t = Workspace:FindFirstChildOfClass("Terrain")
    if t then t.WaterWaveSize=0; t.WaterReflectance=0; t.WaterTransparency=1 end
    
    -- Nuke Workspace
    for _, v in ipairs(Workspace:GetDescendants()) do
        if v:IsA("ParticleEmitter") or v:IsA("Trail") or v:IsA("Beam") then v.Enabled = false
        elseif v:IsA("Decal") or v:IsA("Texture") then v.Transparency = 1
        elseif v:IsA("BasePart") then v.CastShadow = false; v.Material = Enum.Material.Plastic; v.Reflectance = 0
        end
    end
    
    settings().Rendering.QualityLevel = 1
    if setfpscap then setfpscap(Config.PotatoFPS) end
end

local function TogglePotato(state)
    Config.PotatoModeEnabled = state
    SaveSettings()
    if state then
        ApplyPotato()
        PotatoLoop = task.spawn(function() while Config.PotatoModeEnabled do ApplyPotato() task.wait(8) end end)
        WindUI:Notify({ Title = "Graphics", Content = "Extreme Potato Mode ON", Icon = "cpu" })
    else
        if PotatoLoop then task.cancel(PotatoLoop) end
        if setfpscap then setfpscap(60) end
        WindUI:Notify({ Title = "Graphics", Content = "Disabled (Rejoin to fully restore)", Icon = "check" })
    end
end

-- ==========================================================
--  [FEATURE] AUTO 9 TOTEM
-- ==========================================================
local AUTO_9_TOTEM_ACTIVE = false
local AUTO_9_TOTEM_THREAD = nil
local TotemToggleUI = nil 

local TOTEM_DATA = { ["Luck Totem"] = {Id = 1}, ["Mutation Totem"] = {Id = 2}, ["Shiny Totem"] = {Id = 3} }
local REF_CENTER = Vector3.new(93.932, 9.532, 2684.134)
local REF_SPOTS = {
    Vector3.new(45.0468, 9.516, 2730.190), Vector3.new(145.644, 9.516, 2721.907), Vector3.new(84.640, 10.217, 2636.057),
    Vector3.new(45.0468, 110.516, 2730.190), Vector3.new(145.644, 110.516, 2721.907), Vector3.new(84.640, 111.217, 2636.057),
    Vector3.new(45.0468, -92.483, 2730.190), Vector3.new(145.644, -92.483, 2721.907), Vector3.new(84.640, -93.782, 2636.057),
}

local function GetTotemUUID(name)
    local r = Replion:WaitReplion("Data", 5)
    if not r then return nil end
    local s, data = pcall(function() return r:GetExpect("Inventory") end)
    if s and data.Totems then 
        for _, item in ipairs(data.Totems) do 
            if tonumber(item.Id) == TOTEM_DATA[name].Id and (item.Count or 1) >= 1 then return item.UUID end 
        end 
    end
    return nil
end

-- V3 Physics
local stateConnection = nil
local function EnableV3Physics()
    local char = LocalPlayer.Character
    local hum = char:FindFirstChild("Humanoid")
    local root = char:FindFirstChild("HumanoidRootPart")
    if not root or not hum then return end
    
    if char:FindFirstChild("Animate") then char.Animate.Disabled = true end
    hum.PlatformStand = true 
    
    if not stateConnection then
        stateConnection = RunService.Heartbeat:Connect(function()
            if hum and AUTO_9_TOTEM_ACTIVE then hum:ChangeState(Enum.HumanoidStateType.Swimming) end
        end)
    end
    
    local bg = root:FindFirstChild("FlyGuiGyro") or Instance.new("BodyGyro", root)
    bg.Name="FlyGuiGyro"; bg.P=9e4; bg.maxTorque=Vector3.new(9e9,9e9,9e9); bg.CFrame=root.CFrame
    local bv = root:FindFirstChild("FlyGuiVelocity") or Instance.new("BodyVelocity", root)
    bv.Name="FlyGuiVelocity"; bv.velocity=Vector3.zero; bv.maxForce=Vector3.new(9e9,9e9,9e9)
    
    task.spawn(function()
        while AUTO_9_TOTEM_ACTIVE and char do
            for _, v in ipairs(char:GetDescendants()) do if v:IsA("BasePart") then v.CanCollide=false end end
            task.wait(0.1)
        end
    end)
end

local function DisableV3Physics()
    local char = LocalPlayer.Character
    if not char then return end
    local root = char:FindFirstChild("HumanoidRootPart")
    if root then
        if root:FindFirstChild("FlyGuiGyro") then root.FlyGuiGyro:Destroy() end
        if root:FindFirstChild("FlyGuiVelocity") then root.FlyGuiVelocity:Destroy() end
        root.Velocity = Vector3.zero
    end
    local hum = char:FindFirstChild("Humanoid")
    if hum then hum.PlatformStand=false; hum:ChangeState(Enum.HumanoidStateType.GettingUp) end
    if stateConnection then stateConnection:Disconnect(); stateConnection=nil end
    if char:FindFirstChild("Animate") then char.Animate.Disabled=false end
end

local function FlyPhysicsTo(targetPos)
    local root = LocalPlayer.Character.HumanoidRootPart
    local bv = root:FindFirstChild("FlyGuiVelocity")
    local bg = root:FindFirstChild("FlyGuiGyro")
    if not bv or not bg then EnableV3Physics(); bv=root.FlyGuiVelocity; bg=root.FlyGuiGyro end
    
    while AUTO_9_TOTEM_ACTIVE do
        local dist = (targetPos - root.Position).Magnitude
        bg.CFrame = CFrame.lookAt(root.Position, targetPos)
        if dist < 1.5 then bv.velocity=Vector3.zero; break else bv.velocity=(targetPos - root.Position).Unit * 80 end
        RunService.Heartbeat:Wait()
    end
end

local function Run9TotemLoop()
    if AUTO_9_TOTEM_THREAD then task.cancel(AUTO_9_TOTEM_THREAD) end
    AUTO_9_TOTEM_THREAD = task.spawn(function()
        local uuid = GetTotemUUID(Config.TotemType)
        if not uuid then 
            WindUI:Notify({ Title = "Error", Content = "No " .. Config.TotemType .. " found.", Icon = "alert-triangle" })
            if TotemToggleUI then TotemToggleUI:Set(false) end
            return 
        end
        
        local startPos = LocalPlayer.Character.HumanoidRootPart.Position
        if RF_EquipOxygenTank then pcall(function() RF_EquipOxygenTank:InvokeServer(105) end) end
        EnableV3Physics()
        
        for i, refSpot in ipairs(REF_SPOTS) do
            if not AUTO_9_TOTEM_ACTIVE then break end
            FlyPhysicsTo(startPos + (refSpot - REF_CENTER))
            task.wait(0.6)
            
            uuid = GetTotemUUID(Config.TotemType)
            if uuid then
                pcall(function() RE_SpawnTotem:FireServer(uuid) end)
                task.spawn(function() for k=1,5 do RE_EquipToolFromHotbar:FireServer(1) task.wait(0.1) end end)
                WindUI:Notify({ Title = "Totem", Content = "Placed #"..i, Duration = 1 })
            else break end
            task.wait(1.5)
        end
        
        if AUTO_9_TOTEM_ACTIVE then FlyPhysicsTo(startPos) end
        if RF_UnequipOxygenTank then pcall(function() RF_UnequipOxygenTank:InvokeServer() end) end
        DisableV3Physics()
        AUTO_9_TOTEM_ACTIVE = false
        if TotemToggleUI then TotemToggleUI:Set(false) end
        WindUI:Notify({ Title = "Done", Content = "Sequence Finished", Icon = "check" })
    end)
end

-- ==========================================================
--  [FEATURE] MERCHANT
-- ==========================================================
local MerchUpdateThread = nil
local AutoBuyMerchThread = nil

local function GetMerchantDetails()
    local r = Replion:WaitReplion("Merchant", 5)
    if not r then return {} end
    local items = {}
    local MarketData = require(ReplicatedStorage.Shared.MarketItemData)
    if r.Data and r.Data.Items then
        for _, id in ipairs(r.Data.Items) do
            local d = nil
            for _, md in ipairs(MarketData) do if md.Id == id then d = md; break end end
            if d then
                local name = d.Identifier or "Unknown"
                if ItemUtility then
                    pcall(function() 
                        local idata = ItemUtility:GetItemDataFromItemType(d.Type, d.Identifier)
                        if idata and idata.Data then name = idata.Data.Name end
                    end)
                end
                table.insert(items, {Name = name, ID = id, Price = d.Price})
            end
        end
    end
    return items
end

local function RunAutoBuyMerch()
    if AutoBuyMerchThread then task.cancel(AutoBuyMerchThread) end
    AutoBuyMerchThread = task.spawn(function()
        while Config.MerchantAutoBuy do
            for _, item in ipairs(GetMerchantDetails()) do
                pcall(function() RF_PurchaseMarketItem:InvokeServer(item.ID) end)
                task.wait(0.5)
            end
            task.wait(2)
        end
    end)
end

-- ==========================================================
--  [FEATURE] BLATANT FISHING
-- ==========================================================
local BlatantThread = nil
local EquipThread = nil

local function ToggleBlatant(state)
    _G.EnochHub_BlatantActive = state
    Config.BlatantEnabled = state
    SaveSettings()
    
    if state then
        if RF_UpdateAutoFishingState then pcall(function() RF_UpdateAutoFishingState:InvokeServer(true) end) end
        
        BlatantThread = task.spawn(function()
            while Config.BlatantEnabled do
                task.spawn(function()
                    local root = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
                    if root then
                        local s = os.clock()
                        pcall(function() RF_ChargeFishingRod:InvokeServer(os.time()+os.clock()) end)
                        pcall(function() RF_RequestFishingMinigameStarted:InvokeServer(root.Position.X, root.Position.Z) end)
                        local w = Config.CatchDelay - (os.clock() - s)
                        if w > 0 then task.wait(w) end
                        pcall(function() RE_FishingCompleted:FireServer() end)
                        task.wait(Config.CancelDelay)
                        pcall(function() RF_CancelFishingInputs:InvokeServer() end)
                    end
                end)
                task.wait(Config.LoopInterval)
            end
        end)
        
        EquipThread = task.spawn(function()
            while Config.BlatantEnabled do
                if Config.AutoEquip and RE_EquipToolFromHotbar then pcall(function() RE_EquipToolFromHotbar:FireServer(1) end) end
                task.wait(0.5)
            end
        end)
        WindUI:Notify({ Title = "Blatant", Content = "Active", Icon = "zap" })
    else
        if BlatantThread then task.cancel(BlatantThread) end
        if EquipThread then task.cancel(EquipThread) end
        if RF_UpdateAutoFishingState then pcall(function() RF_UpdateAutoFishingState:InvokeServer(false) end) end
        pcall(function() RE_EquipToolFromHotbar:FireServer(0) end)
        WindUI:Notify({ Title = "Blatant", Content = "Disabled", Icon = "x" })
    end
end


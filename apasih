    ["Volcano"]={Pos=Vector3.new(-605.1, 19.5, 160.0),Look=Vector3.new(0.8, 0, 0.5)},
    ["Weather Machine"]={Pos=Vector3.new(-1518.5, 2.9, 1916.1),Look=Vector3.new(0, 0, 1)}
}
local AreaList = {} for n in pairs(Coords) do table.insert(AreaList, n) end table.sort(AreaList)
SectTp:Dropdown({ Title = "Select", Values = AreaList, Callback = function(n) 
    local d = Coords[n]
    if d and LocalPlayer.Character then
        if RE_EquipToolFromHotbar then pcall(function() RE_EquipToolFromHotbar:FireServer(0) end) end
        task.wait(0.3)
        local look = Vector3.new(d.Look.X, 0, d.Look.Z)
        LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.lookAt(d.Pos + Vector3.new(0,3,0), d.Pos + Vector3.new(0,3,0) + look)
    end
end})

-- 7. UTIL
local SectUtil = TabUtil:Section({ Title = "System", TextSize = 17 })
local function ToggleAutoSell(s) Config.AutoSellEnabled=s; SaveSettings(); if s then task.spawn(function() while Config.AutoSellEnabled do if RF_SellAllItems then pcall(function() RF_SellAllItems:InvokeServer() end) end task.wait(60) end end) end end
local function ToggleFav(s) Config.AutoFavRubyEnabled=s; SaveSettings(); if s then task.spawn(function() while Config.AutoFavRubyEnabled do --[[Scan logic omitted for brevity]] task.wait(3) end end) end end
local function ToggleAntiAfk(s) Config.AntiAfkEnabled=s; SaveSettings(); if s then LocalPlayer.Idled:Connect(function() VirtualUser:CaptureController(); VirtualUser:ClickButton2(Vector2.new()) end) end end

SectUtil:Toggle({ Title="Auto Sell", Value=Config.AutoSellEnabled, Callback=ToggleAutoSell })
SectUtil:Toggle({ Title="Auto Fav Ruby", Value=Config.AutoFavRubyEnabled, Callback=ToggleFav })
SectUtil:Toggle({ Title="Anti-AFK", Value=Config.AntiAfkEnabled, Callback=ToggleAntiAfk })
SectUtil:Button({ Title="RESET CONFIG", Callback=function() ResetConfig(); WindUI:Notify({Title="Reset", Content="Config wiped.", Icon="trash"}) end })

if Config.BlatantEnabled then ToggleBlatant(true) end
if Config.WebhookEnabled then ToggleMonitoring(true) end
if Config.PotatoModeEnabled then TogglePotato(true) end
if Config.MerchantAutoBuy then RunAutoBuyMerch() end

WindUI:Notify({ Title = "EnochHub", Content = "Ultimate Loaded.", Duration = 4, Icon = "check" })

-- ==========================================================
-- [[ FIXED ]] GREEN WORLD MODE (Quest Logic Safe)
-- ==========================================================
local function ActivateGreenMode()
    local GreenColor = Color3.fromRGB(0, 255, 0) 
    local Material = Enum.Material.SmoothPlastic
    
    local QuestFolder = workspace:WaitForChild("JUNGLE INTERACTIONS", 5)

    -- 1. SETTING RENDERING
    settings().Rendering.QualityLevel = 1
    game:GetService("Lighting").GlobalShadows = false
    game:GetService("Lighting").FogEnd = 9e9
    game:GetService("Lighting").Brightness = 0
    for _, v in pairs(game:GetService("Lighting"):GetChildren()) do
        if v:IsA("PostEffect") or v:IsA("Sky") or v:IsA("Atmosphere") then v:Destroy() end
    end
    
    -- Helper: Mengubah Part jadi Hijau
    local function Greenify(v)
        if QuestFolder and v:IsDescendantOf(QuestFolder) then return end -- Skip Quest Items

        if v:IsA("BasePart") and not v:IsA("Terrain") then
            v.Material = Material
            v.Color = GreenColor
            v.Reflectance = 0
            v.CastShadow = false
            if v:IsA("MeshPart") then v.TextureID = "" end
        elseif v:IsA("Decal") or v:IsA("Texture") then
            v:Destroy()
        elseif v:IsA("ParticleEmitter") or v:IsA("Trail") or v:IsA("Beam") then
            v.Enabled = false 
        end
    end

    -- 2. GREENIFY MAP
    task.spawn(function()
        for _, v in pairs(workspace:GetDescendants()) do
            Greenify(v)
        end
        workspace.DescendantAdded:Connect(function(v)
            Greenify(v)
        end)
    end)

    -- 3. GREENIFY PLAYER
    local function GreenifyCharacter(character)
        if not character then return end
        task.wait(0.5) 
        for _, v in pairs(character:GetDescendants()) do
            if v:IsA("Clothing") or v:IsA("ShirtGraphic") or v:IsA("Accessory") or v:IsA("CharacterMesh") then
                v:Destroy()
            elseif v:IsA("BasePart") then
                v.Material = Material
                v.Color = GreenColor
                v.Transparency = 0 
            end
        end
        character.ChildAdded:Connect(function(child)
            if child:IsA("Tool") then
                task.wait(0.1)
                for _, part in pairs(child:GetDescendants()) do
                    Greenify(part) 
                end
            end
        end)
    end
    GreenifyCharacter(lp.Character)
    lp.CharacterAdded:Connect(GreenifyCharacter)

    -- 4. MATIKAN AIR
    local Terrain = workspace:FindFirstChildOfClass("Terrain")
    if Terrain then
        Terrain.WaterWaveSize = 0
        Terrain.WaterWaveSpeed = 0
        Terrain.WaterTransparency = 1 
    end
end

-- ===============================
-- DISABLE AUDIO
-- ===============================
local function DisableAudio()
    local SoundService = game:GetService("SoundService")
    RunService.RenderStepped:Connect(function() SoundService.Volume = 0 end)
    local function MuteSound(instance)
        if instance:IsA("Sound") then
            instance.Volume = 0
            instance.Looped = false
            if instance.Playing then instance:Stop() end
        end
    end
    for _, v in pairs(game:GetDescendants()) do pcall(MuteSound, v) end
    game.DescendantAdded:Connect(function(v) pcall(MuteSound, v) end)
end

-- ===============================
-- DISABLE NATIVE AUTO FISH (BRUTAL MODE)
-- ===============================
local function DisableNativeAutoFish()
    local success, AutoFishCtrl = pcall(function() 
        return require(game:GetService("ReplicatedStorage").Controllers.AutoFishingController) 
    end)
    
    if success and AutoFishCtrl then
        if AutoFishCtrl.Stop then AutoFishCtrl:Stop() end
        if AutoFishCtrl.Disable then AutoFishCtrl:Disable() end
        AutoFishCtrl.Start = function() return end 
    end

    local s2, FishingController = pcall(function() 
        return require(game:GetService("ReplicatedStorage").Controllers.FishingController) 
    end)

    if s2 and FishingController then
        FishingController.RequestChargeFishingRod = function() end
        FishingController.SendFishingRequestToServer = function() return false end
    end
    print("[-] Native Controller Disabled (Brutal Mode)")
end


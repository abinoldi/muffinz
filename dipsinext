-- INSTANT FISHING DELAY
-- ===============================
local ROD_DELAYS = {
    [79]  = 4.6,  -- Luck Rod
    [76]  = 4.35, -- Carbon Rod
    [85]  = 4.35, -- Grass Rod
    [77]  = 4.35, -- Demascus Rod
    [78]  = 4.35, -- Ice Rod
    [4]   = 4.35, -- Lucky Rod
    [80]  = 3,    -- Midnight Rod
    [6]   = 2.7,  -- Steampunk Rod
    [7]   = 2.5,  -- Chrome Rod
    [255] = 2.5,  -- Flourescent Rod
    [5]   = 2,    -- Astral Rod
    [126] = 1.7,  -- Ares Rod
    [168] = 1.6,  -- Angler Rod
    [169] = 1.23, -- Ghostfin Rod
    [257] = 1.0,  -- Element Rod
}

-- ===============================
-- INSTANT FISHING LOGIC
-- ===============================
local function RunInstantFish()
    local currentDelay = ROD_DELAYS[STATE.CurrentRodID] or 5
    STATE.Status = "Instant (" .. currentDelay .. "s)"
    pcall(function() RF_UpdateAutoFishingState:InvokeServer(false) end)
    local timestamp = os.time() + os.clock()
    pcall(function() RF_ChargeFishingRod:InvokeServer(timestamp) end)
    pcall(function() RF_RequestFishingMinigameStarted:InvokeServer(-139.63, 0.99) end)
    task.wait(currentDelay)
    pcall(function() RE_FishingCompleted:InvokeServer() end)
    pcall(function() RF_CancelFishingInputs:InvokeServer() end)
end

